<% provide(:title, "working_employee") %>
<div>
  <h1>出勤中社員一覧</h1>
  <table class="table table-bordered table-striped table-condensed">
    <thead>
      <tr>
        <th class="center">社員番号</th>
        <th class="center">名前</th>
      </tr>
    </thead>
    <tbody>
      <% @working_users.each do |working_user|%>
        <tr>
          <td class="center"><%= working_user.user.employee_number %></td>
          <td class="center"><%= working_user.user.name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>